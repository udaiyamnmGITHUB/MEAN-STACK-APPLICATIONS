{
  "name": "gulp-format-md",
  "description": "Gulp plugin for beautifying markdown using pretty-remarkable.",
  "version": "0.1.9",
  "homepage": "https://github.com/jonschlinkert/gulp-format-md",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "jonschlinkert/gulp-format-md",
  "bugs": {
    "url": "https://github.com/jonschlinkert/gulp-format-md/issues"
  },
  "license": "MIT",
  "files": [
    "index.js",
    "lib"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "extend-shallow": "^2.0.1",
    "get-value": "^2.0.5",
    "lazy-cache": "^1.0.4",
    "plugin-error": "^0.1.2",
    "pretty-remarkable": "^0.3.1",
    "remarkable": "^1.6.2",
    "sections": "^0.1.8",
    "through2": "^2.0.1"
  },
  "devDependencies": {
    "gulp": "^3.9.1",
    "gulp-eslint": "^2.0.0",
    "gulp-istanbul": "^0.10.4",
    "gulp-mocha": "^2.2.0",
    "mocha": "^2.4.5"
  },
  "keywords": [
    "beautify",
    "format",
    "formatter",
    "gulp",
    "gulpplugin",
    "markdown",
    "md",
    "prettify",
    "pretty",
    "remarkable"
  ],
  "verb": {
    "related": {
      "list": [
        "gulp",
        "pretty-remarkable",
        "remarkable"
      ]
    },
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "toc": false,
    "lint": {
      "reflinks": true
    },
    "reflinks": [
      "verb"
    ]
  }
}
